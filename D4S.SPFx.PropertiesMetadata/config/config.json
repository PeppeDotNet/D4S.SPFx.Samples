{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sample/SampleWebPart.js",
          "manifest": "./src/webparts/sample/SampleWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-client-custom-fields": "node_modules/sp-client-custom-fields/dist/sp-client-custom-fields.bundle.js"
  },
  "localizedResources": {
    "SampleWebPartStrings": "lib/webparts/sample/loc/{locale}.js",
    "sp-client-custom-fields/strings": "node_modules/sp-client-custom-fields/lib/loc/{locale}.js"
  }
}
